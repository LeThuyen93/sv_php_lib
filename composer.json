{
  "name": "sieuviet/php-lib",
  "description": "A common php libraries",
  "type": "library",
  "autoload": {
    "psr-4": {
      "CodeBase\\": "src/phalcon/"
    }
  },
  "require": {
    "mobiletech/php-lib": "^1.0",
    "igsem/phalcon-swagger": "^0.0.2",
    "firebase/php-jwt": "^5.0",
    "phalcon/incubator": "^3.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8",
    "phalcon/incubator": "^3"
  },
  "version": "1.0.0",
  "minimum-stability": "dev",
  "config": {
    "preferred-install": "dist",
    "optimize-autoloader": true,
    "secure-http": false
  },
  "repositories": [
    {
      "type": "git",
      "url": "http://git.mobiletech.vn/MobileTech/php-lib.git",
      "reference": "master"
    }
  ]
}
